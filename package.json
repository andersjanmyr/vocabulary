{
  "name": "vocabulary",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git@github.com:andersjanmyr/vocabulary.git"
  },
  "scripts": {
    "build-src": "browserify -t coffeeify --extension='.coffee' app/src/main.coffee | uglifyjs -mc > static/bundle.js",
    "build-less": "lessc app/less/main.less static/main.css",
    "build": "npm run build-src && npm run build-less",
    "watch-src": "watchify app/src/main.coffee -t coffeeify --extension='.coffee' -o static/bundle.js -dv",
    "watch-less": "nodemon --watch app/less/main.less --ext less --exec 'npm run' build-less",
    "start": "bin/server",
    "watch-server": "nodemon --watch lib --ext .coffee bin/server",
    "watch": "npm run watch-src & npm run watch-less & npm run watch-server",
    "test": "mocha --reporter spec test",
    "deploy": "./bin/deploy.sh"
  },
  "dependencies": {
    "body-parser": "^1.0.0",
    "coffee-script": "^1.8.0",
    "cookie-parser": "^1.3.3",
    "debug": "^0.7.4",
    "express": "^4.10.2",
    "morgan": "^1.5.0"
  },
  "devDependencies": {
    "browserify": "^3.46.1",
    "coffeeify": "^0.6.0",
    "envify": "^1.2.1",
    "jshint": "^2.5.10",
    "less": "^1.7.5",
    "moment": "^2.8.3",
    "nodemon": "^1.2.1",
    "react": "^0.9.0",
    "react-tools": "^0.9.0",
    "reactify": "^0.5.1",
    "uglify-js": "^2.4.15",
    "watchify": "^0.6.4"
  }
}
