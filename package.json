{
  "name": "vocabulary",
  "version": "1.0.1",
  "private": true,
  "engines": {
    "node": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:andersjanmyr/vocabulary.git"
  },
  "scripts": {
    "build-src": "browserify -t coffeeify --extension='.coffee' -t literalify app/src/main.coffee | uglifyjs -mc > static/bundle.js",
    "build-less": "lessc app/less/main.less static/main.css",
    "build": "npm run build-src && npm run build-less",
    "watch-src": "watchify app/src/main.coffee -t coffeeify --extension='.coffee' -t literalify -o static/bundle.js -dv",
    "watch-less": "nodemon --watch app/less/main.less --ext less --exec 'npm run' build-less",
    "start": "bin/server",
    "watch-server": "nodemon --watch lib --ext .coffee bin/server",
    "watch": "npm run watch-src & npm run watch-less & npm run watch-server",
    "test": "mocha --recursive --compilers coffee:coffee-script/register --reporter spec test",
    "deploy": "./bin/deploy.sh"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "browserify": "^13.1.0",
    "coffee-script": "^1.11.1",
    "coffeeify": "^2.0.1",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^1.2.0",
    "debug": "^2.2.0",
    "ejs": "^1.0.0",
    "envify": "^3.4.1",
    "express": "^4.14.0",
    "jshint": "^2.9.3",
    "less": "^2.7.1",
    "livereload": "^0.3.7",
    "lodash": "^2.4.2",
    "mocha": "^2.5.3",
    "mongodb": "^1.4.0",
    "mongoskin": "^1.4.13",
    "morgan": "^1.7.0",
    "nodemon": "^1.11.0",
    "passport": "^0.2.2",
    "passport-google-oauth": "^0.1.5",
    "react": "^15.3.2",
    "react-tools": "^0.13.3",
    "reactify": "^1.1.1",
    "sinon": "^1.17.6",
    "uglify-js": "^2.7.3",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "browserify": "*",
    "chai": "^1.10.0",
    "coffeeify": "*",
    "envify": "*",
    "jshint": "*",
    "less": "*",
    "literalify": "^0.3.0",
    "livereload": "^0.3.4",
    "mocha": "^2.0.1",
    "nodemon": "*",
    "react": "*",
    "react-tools": "*",
    "reactify": "*",
    "route-parser": "0.0.2",
    "sinon": "^1.12.1",
    "supertest": "^0.15.0",
    "uglify-js": "*",
    "watchify": "*"
  },
  "literalify": {
    "react": "window.React",
    "jquery": "window.$"
  }
}
